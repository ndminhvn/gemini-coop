# Gemini API Key (Required)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# JWT Secret Key for authentication (Required)
# Generate a secure random string for production
SECRET_KEY=scret_key_change_this_in_production

# Database Configuration
# For PostgreSQL (recommended for production):
DATABASE_URL=postgresql://gemini_user:gemini_password@localhost:5433/gemini_coop

# For SQLite (development/testing):
# DATABASE_URL=sqlite:///./gemini_coop.db

# JWT Configuration
SECRET_KEY=your-secret-key-here-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Gemini API
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-2.5-flash

# Server Configuration
HOST=0.0.0.0
PORT=8000

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
